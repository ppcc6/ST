print("Enter three numbers:")
a = int(input())
b = int(input())
c = int(input())

if a < (b + c) and b < (a + c) and c < (a + b):
    if a == b == c:
        print("Equilateral")
    elif a != b and b != c and a != c:
        print("Scalene")
    else:
        print("Isosceles")
else:
    print("Values cannot form a triangle")
